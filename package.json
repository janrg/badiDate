  "module": "dist/localBadiDate-msm-locales.m.js",
  "types": "dist/badiDate.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint --ext .js --ext .ts .",
    "rollup": "fancy-rollup && node ./dist-rename.js",
